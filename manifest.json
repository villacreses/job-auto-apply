{
  "name": "Job Auto Apply",
  "description": "The competition for jobs is getting tough. Modern problems require modern solutions.",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
    "default_popup": "popup.html",
    "default_icon": "default.png"
  },
  "permissions": ["scripting", "activeTab", "storage"],
  "web_accessible_resources": [
    {
      "resources": ["assets/injected/*.js"],
      "matches": ["https://*/*"]
    }
  ], 
  "host_permissions": [
    "https://*/*"
  ]
}